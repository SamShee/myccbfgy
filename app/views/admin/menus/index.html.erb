<p id="notice"><%= notice %></p>

<h1>Admin Menus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th>Icon</th>
      <th>Icon location</th>
      <th>Created by</th>
      <th>Updated by</th>
      <th>Enabled</th>
      <th>Is enabled</th>
      <th>Is locked</th>
      <th>Is expired</th>
      <th>Status</th>
      <th>Type</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_menus.each do |admin_menu| %>
      <tr>
        <td><%= admin_menu.name %></td>
        <td><%= admin_menu.url %></td>
        <td><%= admin_menu.icon %></td>
        <td><%= admin_menu.icon_location %></td>
        <td><%= admin_menu.created_by %></td>
        <td><%= admin_menu.updated_by %></td>
        <td><%= admin_menu.enabled %></td>
        <td><%= admin_menu.is_enabled %></td>
        <td><%= admin_menu.is_locked %></td>
        <td><%= admin_menu.is_expired %></td>
        <td><%= admin_menu.status %></td>
        <td><%= admin_menu.type %></td>
        <td><%= admin_menu.parent_id %></td>
        <td><%= link_to 'Show', admin_menu %></td>
        <td><%= link_to 'Edit', edit_admin_menu_path(admin_menu) %></td>
        <td><%= link_to 'Destroy', admin_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Menu', new_admin_menu_path %>
